{
    "version": "1.0",
    "plugin_name": "netbox_licences",

    "class_templates": {
        "named": {
            "extends": [
                "netbox_rapidcrud.models.predefined.NamedModel"
            ]
        }
    },

    "classes": {
        "SoftwareProvider": {
            "class_templates": [
                "named"
            ],
            "fields": [
                "full_name"
            ]
        },
        "SoftwareType": {
            "extends": [
                "netbox_rapidcrud.models.predefined.NamedModel"
            ]
        },
        "Software": {
            "class_templates": [
                "named"
            ],
            "fields": [
                {
                    "name": "provider",
                    "type": "ForeignKey",
                    "params": {
                        "on_delete": "CASCADE",
                        "to": "SoftwareProvider"
                    }
                },
                {
                    "name": "software_type",
                    "type": "ForeignKey",
                    "params": {
                        "on_delete": "CASCADE",
                        "to": "SoftwareType"
                    }
                }
            ]
        }
    }
}
